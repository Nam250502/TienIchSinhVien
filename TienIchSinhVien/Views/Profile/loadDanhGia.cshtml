@model IEnumerable<TienIchSinhVien.Models.DanhGiaUser>

@{
    Layout = null;

}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>loadDanhGia</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
</head>
<body>
    <table>
        @foreach (var item in Model)
        {
           
        <div class="container">
            <div class="row">
                <div class="col-md-2">
                    <img src="@item.Anh" alt="Avatar" class="img-fluid rounded-circle avatar" style="width: 100px; height: 100px; object-fit: cover;">
                </div>
                <div class="col-md-5">
                    <h4>@item.Name</h4>
       
                    <div class="card">
                        <div class="card-body">
                            <p class="card-text">
                                @item.DanhGia
                                
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        }

    </table>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
</body>
</html>
