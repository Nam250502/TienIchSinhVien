
@{
    ViewBag.Title = "GetUserInfo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Các phòng trọ đã đăng</h2>

@if (ViewBag.PTdadang.Count() == 0)
{
    <p>Không có phòng trọ nào đã đăng.</p>
}
else
{
    <ul>
        @foreach (var phongTro in ViewBag.PTdadang)
        {
            <li>
                <a href="@Url.Action("ChiTietPhongTro", "PhongTro", new { id = phongTro.Id })">@phongTro.TenPhongTro</a>
            </li>
        }
    </ul>
}